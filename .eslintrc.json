{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto",
        "trailingComma": "all"
      }
    ],
    "comma-dangle": [2, "always-multiline"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["app"]
      }
    }
  }
}
